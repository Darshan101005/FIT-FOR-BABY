{
  "name": "fit-for-baby",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "npm run build:web",
    "build:web": "expo export --platform web && node ./scripts/copy-pwa-files.js",
    "preview:web": "npx serve dist",
    "deploy:web": "npm run build:web && firebase deploy --only hosting",
    "lint": "expo lint",
    "emulators": "firebase emulators:start",
    "emulators:import": "firebase emulators:start --import=./firebase-data --export-on-exit",
    "deploy:rules": "firebase deploy --only firestore:rules,storage:rules",
    "deploy:indexes": "firebase deploy --only firestore:indexes",
    "generate:pwa-icons": "node ./scripts/generate-pwa-icons.js",
    "generate:app-icons": "node ./scripts/convert-logo-to-icons.js && node ./scripts/generate-android-icons.js"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.27",
    "expo-constants": "~18.0.10",
    "expo-device": "~8.0.10",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-image-picker": "~17.0.9",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.10",
    "expo-notifications": "~0.32.14",
    "expo-router": "~6.0.17",
    "expo-sharing": "~14.0.8",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-updates": "~29.0.15",
    "expo-web-browser": "~15.0.10",
    "firebase": "^12.5.0",
    "firebase-tools": "^14.27.0",
    "react": "19.1.0",
    "react-datepicker": "^8.10.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "@types/react-native": "^0.72.8",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "sharp": "^0.34.5",
    "typescript": "~5.9.2"
  },
  "private": true
}
